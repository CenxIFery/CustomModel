{
  "schemaVersion": 1,
  "id": "custommodel",
  "version": "${version}",

  "name": "Custom Model",
  "description": "Customize your player model in Minecraft using JSON.",
  "authors": [
    "Gamepiaynmo"
  ],
  "contact": {
    "homepage": "https://github.com/Gamepiaynmo",
    "sources": "https://github.com/Gamepiaynmo/CustomModel"
  },

  "license": "CC0-1.0",
  "icon": "assets/custommodel/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [ "com.github.gamepiaynmo.custommodel.server.CustomModel" ],
    "client": [ "com.github.gamepiaynmo.custommodel.client.CustomModelClient" ],
    "modmenu": [ "com.github.gamepiaynmo.custommodel.client.ModMenu" ],
    "cotton-client-commands": ["com.github.gamepiaynmo.custommodel.client.Command"]
  },
  "mixins": [
    "custommodel.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "autoconfig1": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}
